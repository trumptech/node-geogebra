{
  "name": "node-geogebra",
  "version": "1.2.3",
  "description": "Generate graphics with geogebra from nodejs",
  "main": "dist/index.js",
  "scripts": {
    "bundle:esm": "tsc --target es2018 --outDir esm",
    "bundle:cjs": "tsc --target es2015 --module commonjs --outDir dist",
    "bundle": "npm run bundle:esm && npm run bundle:cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmulet/node-geogebra.git"
  },
  "keywords": [
    "geogebra",
    "nodejs",
    "chromium",
    "headless"
  ],
  "author": "Josep Mulet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jmulet/node-geogebra/issues"
  },
  "homepage": "https://github.com/jmulet/node-geogebra#readme",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/puppeteer": "^7.0.4",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "puppeteer": "^21.0.3"
  }
}
